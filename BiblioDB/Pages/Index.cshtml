@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";

    //var data = Model.BookList;
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>

    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.BookList[0].Title)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.BookList[0].PublishYear)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.BookList)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Title)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.PublishYear.Year)
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>